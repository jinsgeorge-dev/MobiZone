            <label>ProductType</label>
        </div>
        @* <select asp-for="parantId" asp-items="new SelectList(ViewBag.EnumList)"></select>*@
        <input asp-for="ParentId" type="hidden" value=1 />
        <div class="form-group form-group mx-sm-3 mb-2">
            <input asp-for=" name" class="form-control" />
        </div>
        <button class="btn btn-primary mb-2">submit</button>
        <div class="nothing"></div>
        <button id="view" class="btn btn-secondary"><a asp-action="@url" asp-route-id=1>View</a></button>
